
public class func {

	static double add(double d,double e) //declaration
	{
		double c;    
		c=d+e;
		System.out.println(c);
		return c;
	}
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		double d=add(10.5,20.5);    //call
		add(1567,2597);
		add(532144,598745);
	}
}

class demo{
	public demo(int a,int b) 
	{
		int x=a;
		int y=b;
		System.out.println("Welcome to my class");
	}
	void show() {
		int c;
		c=x+y;
		System.out.println(c);
	}
}
public class oops {
	public static void main(String[] args) {
		demo  d=new demo(10,20);
		demo  f=new demo(50,30);
		//d.print();
		f.show();
	}
}

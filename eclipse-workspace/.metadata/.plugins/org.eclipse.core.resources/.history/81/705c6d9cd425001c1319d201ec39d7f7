
public class operators {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int a,b,c;
		a=10;
		b=20;
		c=30;
		if((a>b) && (a>c)){
			System.out.println("A is Big");
		}
		else if(b>c) {
			System.out.println("B is Big");
		}
		else {
			System.out.println("C is Big");
		}
	}

}

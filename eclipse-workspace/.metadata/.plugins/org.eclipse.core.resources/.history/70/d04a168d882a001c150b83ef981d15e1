class demo{
	public int x,y;
	public demo(int a,int b) 
	{
		this.x=a;
		this.y=b;
		System.out.println("Welcome to my class");
	}
	void show() {
		int c;
		c=this.x+this.y;
		System.out.println(c);
	}
}
public class oops {
	public static void main(String[] args) {
		demo  d=new demo(10,20);
		demo  f=new demo(50,30);
		//d.print();
		f.show();
	}
}

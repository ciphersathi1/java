class ask{
	void add(double a,double b) {
		double c;
		c=a+b;
		System.out.println("output is :"+c);		
	}
}
class ask2 extends ask{
		void add(int a,int b) {
			int c;
			c=a+b;
			System.out.println("Ans is :"+c);		
		}		
}

public class ast {
	public static void main(String[] args) {
		ask2 a=new ask2();
		a.add(10.54,20.1);
		
	}

}
